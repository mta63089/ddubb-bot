{
  "name": "ddubb-bot",
  "version": "0.0.1",
  "private": "true",
  "description": "ddubb discord bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "NODE_ENV=production node build/bot.js",
    "prod": "tsc -p . && NODE_ENV=production node build/bot.js",
    "dev": "nodemon --exec ts-node src/bot.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mta630/ddubb-bot.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "discord",
    "ddubb",
    "ddubb.net",
    "administration",
    "server",
    "management"
  ],
  "author": "mta630",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mta630/ddubb-bot/issues"
  },
  "homepage": "https://github.com/mta630/ddubb-bot#readme",
  "dependencies": {
    "axios": "^1.5.1",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.10",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
